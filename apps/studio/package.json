  "type": "module",
  "main": "./out/main/index.js",
  "files": [],
  "scripts": {
    "build": "electron-vite build",
    "build:analyze": "ANALYZE_BUILD=true pnpm run build",
    "build:dev": "electron-vite build --mode development --sourcemap",
    "build:install-app-deps": "electron-builder install-app-deps",
    "build:linux": "pnpm run build:pre-builder && electron-builder --linux",
    "build:linux:arm64": "pnpm run build:pre-builder && electron-builder --linux --arm64",
    "build:linux:x64": "pnpm run build:pre-builder && electron-builder --linux --x64",
    "build:mac": "pnpm run build:pre-builder && electron-builder --mac",
    "build:mac:arm64": "pnpm run build:pre-builder && electron-builder --mac --arm64",
    "build:mac:dev": "pnpm run build:pre-builder:dev && electron-builder --mac",
    "build:mac:local": "op run --no-masking --env-file=./.env.build --env-file=./.env.production -- pnpm run build:mac",
    "build:mac:x64": "pnpm run build:pre-builder && electron-builder --mac --x64",
    "build:pre-builder": "pnpm run build:install-app-deps && pnpm run build",
    "build:pre-builder:dev": "pnpm run build:install-app-deps && pnpm run build:dev",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:windows": "pnpm run build:pre-builder && electron-builder --win",
    "build:windows:arm64": "pnpm run build:pre-builder && electron-builder --win --arm64",
    "build:windows:dev": "pnpm run build:pre-builder:dev && electron-builder --win",
    "build:windows:x64": "pnpm run build:pre-builder && electron-builder --win --x64",
    "check:lint": "eslint . --max-warnings 0 --cache",
    "check:types": "tsc --noEmit",
    "dev": "electron-vite dev",
    "dev:fresh": "ELECTRON_USE_NEW_USER_FOLDER=true pnpm run dev",
    "electron-vite": "electron-vite",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "start": "electron-vite preview",
    "tag:release:minor": "tsx scripts/tag-release.ts minor",
    "tag:release:patch": "tsx scripts/tag-release.ts patch",
    "tail-logs:dev": "tail -f ~/Library/Logs/Quests/main.log",
    "tail-logs:prod": "tail -f ~/Library/Application\\ Support/Quests/logs/main.log",
    "test": "vitest",
    "turbo:check": "turbo run check:types check:lint",
    "turbo:check-and-test": "turbo run check:types check:lint test:ci",
    "turbo:test": "turbo run test test:ci"
  },
