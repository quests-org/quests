  "scripts": {
    "build": "electron-vite build",
    "build:analyze": "ANALYZE_BUILD=true pnpm run build",
    "build:dev": "electron-vite build --mode development --sourcemap",
    "build:env:unsigned": "pnpm run build:install-and-build && electron-builder --config.mac.identity=null --config.win.signAndEditExecutable=false --dir",
    "build:install-and-build": "pnpm run build:install-app-deps && pnpm run build",
    "build:install-and-build:dev": "pnpm run build:install-app-deps && pnpm run build:dev",
    "build:install-app-deps": "electron-builder install-app-deps",
    "build:linux": "pnpm run build:install-and-build && electron-builder --linux",
    "build:linux:arm64": "pnpm run build:install-and-build && electron-builder --linux --arm64",
    "build:linux:x64": "pnpm run build:install-and-build && electron-builder --linux --x64",
    "build:mac": "pnpm run build:install-and-build && electron-builder --mac",
    "build:mac:arm64": "pnpm run build:install-and-build && electron-builder --mac --arm64",
    "build:mac:dev": "pnpm run build:install-and-build:dev && electron-builder --mac",
    "build:mac:local": "op run --no-masking --env-file=./.env.build --env-file=./.env.production -- pnpm run build:mac",
    "build:mac:unsigned": "pnpm run build:install-and-build && electron-builder --mac --config.mac.identity=null --dir",
    "build:mac:x64": "pnpm run build:install-and-build && electron-builder --mac --x64",
    "build:test": "electron-vite build --mode test",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win:unsigned": "pnpm run build:install-and-build && electron-builder --win --config.win.signAndEditExecutable=false --dir",
    "build:windows": "pnpm run build:install-and-build && electron-builder --win",
    "build:windows:arm64": "pnpm run build:install-and-build && electron-builder --win --arm64",
    "build:windows:dev": "pnpm run build:install-and-build:dev && electron-builder --win",
    "build:windows:x64": "pnpm run build:install-and-build && electron-builder --win --x64",
    "check:lint": "eslint . --max-warnings 0 --cache",
    "check:types": "tsc --noEmit",
    "dev": "NODE_OPTIONS='--enable-source-maps' electron-vite dev --sourcemap",
    "dev:fresh": "ELECTRON_USE_NEW_USER_FOLDER=true NODE_OPTIONS='--enable-source-maps' electron-vite dev --sourcemap",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "smoke-test": "vitest run --config vitest.smoke.config.ts",
    "start": "electron-vite preview",
    "tag:release:minor": "tsx scripts/tag-release.ts minor",
    "tag:release:minor:beta": "tsx scripts/tag-release.ts minor beta",
    "tag:release:patch": "tsx scripts/tag-release.ts patch",
    "tag:release:patch:beta": "tsx scripts/tag-release.ts patch beta",
    "tail-logs:dev": "tail -f ~/Library/Logs/Quests/main.log",
    "tail-logs:prod": "tail -f ~/Library/Application\\ Support/Quests/logs/main.log",
    "test": "vitest",
    "turbo:check": "turbo run check:types check:lint",
    "turbo:check-and-test": "turbo run check:types check:lint test:ci",
    "turbo:test": "turbo run test test:ci"
  },
