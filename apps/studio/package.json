{
  "name": "@quests/studio",
  "version": "1.3.1",
  "description": "Quests, the open-source app builder",
  "repository": {
    "type": "git",
    "url": "https://github.com/quests-org/quests"
  },
  "author": {
    "name": "Quests",
    "email": "oss@quests.dev"
